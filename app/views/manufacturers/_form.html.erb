<%= form_for(@manufacturer, :html => { :multipart => true}) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :image %>
    <%= f.cl_image_upload :image_url %>

    <%= f.label :founded %>
    <%= f.text_field :founded %>

    <%= f.label :location %>
    <%= f.text_field :location %>

    <%= f.submit %>

<% end %>