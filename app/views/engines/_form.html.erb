<%= form_for(@engine, :html => { :multipart => true}) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :image %>
    <%= f.cl_image_upload :image_url %>

    <%= f.label :capacity %>
    <%= f.text_field :capacity %>

    <%= f.label :hp %>
    <%= f.text_field :hp %>

    <%= f.label :torque %>
    <%= f.text_field :torque %>

    <%= f.label :mod %>
    <%= f.text_field :mod %>

    <%= f.submit %>

<% end %>